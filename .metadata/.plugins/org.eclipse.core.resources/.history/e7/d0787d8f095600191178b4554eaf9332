import java.util.Scanner;

public class Pow {
	int inputNum;
	
	public void change() {
		Scanner scan = new Scanner(System.in);
		int inputNum = 0;
		this.inputNum = scan.nextInt();
		
		int 나머지 = 0;
		int 몫 = 0;
		
		while(true) {
			int index = 0;
			String[] arr = new String[index];
			
			if(this.inputNum>=8) {
				나머지 = this.inputNum%8;
				String 나머지2 = Integer.toString(나머지);
				System.out.print(나머지2);
				arr[index] += 나머지2;
				index += 1;
				
				몫 = this.inputNum/8;
				this.inputNum = 몫;
			} else {
				String inputStr = Integer.toString(this.inputNum);
				System.out.println(inputStr);
				break;
			}		
		}
		// 배열에 넣기
		String[] arr = new String[1];
	}

}
