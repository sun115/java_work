	public void change () {
		Scanner scan = new Scanner(System.in);
//		this.inputNum = scan.nextInt();
		
		while(true) {
			나눈값 = this.inputNum;
			

		if (나눈값>=8) {
			나머지 = this.inputNum % 8;
			나눈값 = ((int)this.inputNum / 8);
			index=0;
			this.arr[index+=1] = 나머지;
			
			
		} else {
			break;
		}
		
		}
		System.out.println(arr);
	}


