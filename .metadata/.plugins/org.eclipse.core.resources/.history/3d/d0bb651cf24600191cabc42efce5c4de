public class Practice_calculate {
	public static void main(String[] args) {
		int a = 10;
		int b = 3;
		boolean c;
		boolean d;
		boolean e;
		
		c = a != b; 
		// c = (a와 b가 다른가? yes=true, no=false)
		System.out.println(c);
		
		d = (a != b) && (a > b);
		// d = (a와b가 다른가?) 그리고 (a는b보다 큰가?) both yes=true, 하나라도 no=false
		System.out.println(d);
	}
	
}