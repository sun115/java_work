import java.util.Random;
import java.util.Scanner;

public class RockScissorPaper {
	public static void main(String[] args) {
		
//	내가 낼 숫자 선택
		Scanner scan = new Scanner(System.in);
		System.out.println("가위바위보를 해 보세요. \n 가위는 0, 바위는 1, 보는 2 입니다.");
		int me = scan.nextInt();
		
//	컴퓨터가 낼 랜덤 숫자 실행
		Random rsp = new Random();
		int com = rsp.nextInt(3);
		
//	내가 낸 숫자를 한글로 바꾸기
		if (me==0) {
			System.out.println("/n나 : 가위");}
		else if (me==1) {
			System.out.println("/n나 : 바위");}
		else {System.out.println("/n나 : 보");}
		
//	컴퓨터가 낸 숫자를 한글로 바꾸기		
		if (com==0) {
			System.out.println("/n컴  : 가위");}
		else if (com==1) {
			System.out.println("/n컴 : 바위");}
		else {System.out.println("/n컴 : 보");}
		
		
		if (com == me) {
			System.out.println("/nDRAW");
		} else if (com==0 && me==1 ) {
			System.out.println("/nWIN");
		} else if (com==0 && me==2) {
			System.out.println("/nLOSE");
		} else if (com==1 && me==0) {
			System.out.println("/nLOSE");
		} else if (com==1 && me==2) {
			System.out.println("/nWIN");
		} else if (com==2 && me==0) {
			System.out.println("/nWIN");
		} else if (com==2 && me==0) {
			System.out.println("/nLOSE");
		}
	

}
}
