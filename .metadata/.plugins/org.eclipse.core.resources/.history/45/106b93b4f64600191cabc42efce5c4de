import java.util.Random;
import java.util.Scanner;

public class RockSissiorPaper {
	public static void main(String[] args) {
		
		Random rsp = new Random();
		
		int com = rsp.nextInt(3);
		int me = rsp.nextInt(3);
		
		Scanner scan = new Scanner(System.in);
		System.out.println("가위바위보를 해 보세요. \n 가위는 0, 바위는 1, 보는 2 입니다."/n);
		//String a = "rock";
		
		System.out.println("컴퓨터 : " + com);
		System.out.println("나 : " + me);
		
		// 가위 = 0, 바위 = 1, 보 = 2
		// 
		if (com == me) {
			System.out.println("무승부");
		} else if (com==0 && me==1 ) {
			System.out.println("이김ㅋ");
		} else if (com==0 && me==2) {
			System.out.println("졌다..");
		} else if (com==1 && me==0) {
			System.out.println("졌다..");
		} else if (com==1 && me==2) {
			System.out.println("이김ㅋ");
		} else if (com==2 && me==0) {
			System.out.println("이김ㅋ");
		} else if (com==2 && me==0) {
			System.out.println("졌다..");
		}
	

}
}
