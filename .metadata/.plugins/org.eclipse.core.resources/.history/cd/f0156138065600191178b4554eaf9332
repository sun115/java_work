import java.util.Scanner;

public class Pow2 {
	int inputNum;
	
	public void change() {
		Scanner scan = new Scanner(System.in);
		int inputNum = 0;
		this.inputNum = scan.nextInt();
		
		int 나머지 = 0;
		int 몫 = 0;
		
		while(true) {
		this.inputNum = 몫;
		if(this.inputNum>=8) {
			나머지 = this.inputNum%8;
			System.out.print(나머지);
			몫 = this.inputNum/8;
		} else {
		System.out.println(this.inputNum);
		break;
		}		}
	}

}
